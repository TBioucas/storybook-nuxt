{
  "name": "core",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint '**/*.{vue,js}' --ignore-path .gitignore",
    "lint:js:fix": "eslint '**/*.{vue,js}' --fix --ignore-path .gitignore",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint:style:fix": "stylelint **/*.{vue,css,scss} --fix --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:js:fix && npm run lint:style:fix",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "version": "echo $npm_package_version",
    "storybook": "nuxt storybook",
    "storybook:build": "nuxt storybook build",
    "storybook:eject": "nuxt storybook eject"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "nuxt": "^2.14.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/storybook": "^3.1.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addon-storyshots": "^6.0.28",
    "@storybook/vue": "^6.0.28",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "react-is": "^17.0.1",
    "storybook-vue-router": "^1.0.7",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "vue-jest": "^3.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run lint:js:fix",
      "git add"
    ],
    "*.{css,scss,vue}": [
      "npm run lint:style:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
